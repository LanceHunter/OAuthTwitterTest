# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'Twitter API OAuth Test' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Twitter API OAuth Test
  pod "AFNetworking"
  pod "BDBOAuth1Manager"
  

  target 'Twitter API OAuth TestTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Twitter API OAuth TestUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
